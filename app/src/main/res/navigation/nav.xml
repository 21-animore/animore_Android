            app:destination="@id/home_fragment" />
    </fragment>

    <fragment
        android:id="@+id/past_card_fragment"
        android:name="org.techtown.animore.PastFragmentMore"
        tools:layout="@layout/fragment_past_card_more"></fragment>



</navigation>