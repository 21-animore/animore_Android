            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out"/>
    </fragment>

    <fragment
        android:id="@+id/add_random_bengal"
        android:name="org.techtown.animore.AddFragementMissionBengal"
        tools:layout="@layout/fragment_add_random_bengal"/>

    <fragment
        android:id="@+id/add_random_harpseal"
        android:name="org.techtown.animore.AddFragementMissionHarpseal"
        tools:layout="@layout/fragment_add_random_harpseal"/>

    <fragment
        android:id="@+id/add_random_illipika"
        android:name="org.techtown.animore.AddFragementMissionIllipika"
        tools:layout="@layout/fragment_add_random_illipika"/>

    <fragment
        android:id="@+id/add_random_java"
        android:name="org.techtown.animore.AddFragementMissionJava"
        tools:layout="@layout/fragment_add_random_java"/>

    <fragment
        android:id="@+id/add_choose_normal_guanicoe"
        android:name="org.techtown.animore.AddFragmentChooseNormalGuanicoe"
        tools:layout="@layout/fragment_choose_option_normal_guanicoe">
        <action
            android:id="@+id/action_add_choose_normal_guanicoe_to_add_choose_continuous_guanicoe"
