<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/home_fragment">

    <fragment
        android:id="@+id/home_fragment"
        android:name="org.techtown.animore.HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_home_fragment_to_add_fragment"
            app:destination="@id/add_fragment" />
        <action
            android:id="@+id/action_home_fragment_to_past_fragment"
            app:destination="@id/past_fragment" />
        <action
            android:id="@+id/action_home_fragment_to_home_more_card_fragment"
            app:destination="@id/home_more_card_fragment" />
    </fragment>

    <fragment
        android:id="@+id/home_more_card_fragment"
        android:name="org.techtown.animore.HomeMoreCardFragment"
        tools:layout="@layout/fragment_home_card_more">
        <action
            android:id="@+id/action_home_more_card_fragment_to_home_fragment"
            app:destination="@id/home_fragment" />
    </fragment>

    <fragment
        android:id="@+id/add_fragment"
        android:name="org.techtown.animore.AddFragment"
        tools:layout="@layout/fragment_add">
        <action
            android:id="@+id/action_add_fragment_to_add_animal_guanicoe"
            app:destination="@id/add_animal_guanicoe"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
        <action
            android:id="@+id/action_add_fragment_to_add_animal_bengal"
            app:destination="@id/add_animal_bengal"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
        <action
            android:id="@+id/action_add_fragment_to_add_animal_java"
            app:destination="@id/add_animal_java"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
        <action
            android:id="@+id/action_add_fragment_to_add_animal_illipika"
            app:destination="@id/add_animal_illipika"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
